{
    "store.custom#politica-de-privacidade": {
        "blocks": ["flex-layout.row#politica-de-privacidade"]
    }, 
    "flex-layout.row#politica-de-privacidade": {
        "title": "Colunas",
        "props": { 
            "horizontalAlign": "between",
            "verticalAlign": "top",
            "marginTop": 8,
            "marginBottom": 9
        },
        "children": [ 
            "flex-layout.col#pp-col1-insti-padrao",
            "flex-layout.col#pp-col2-insti-padrao"
        ]
    }, 
    "flex-layout.col#pp-col1-insti-padrao": {
        "title": "Coluna Menu",
        "props": {
            "blockClass": "col1-insti-padrao",
            "width": "24%"
        },
        "children": [
            "rich-text#menu-institucional"
        ]
    },
    "flex-layout.col#pp-col2-insti-padrao": {
        "title": "Coluna de Texto",
        "props": {
            "blockClass": "pp-col2-insti-padrao",
            "width": "73%"
        },
        "children": [
            "flex-layout.row#pp-header-txt-inst",
            "flex-layout.row#pp-txt-inst-principal"
        ]
    },
    "rich-text#pp-menu-inst-title": {
        "props": {
            "text": "## NAVEGUE ##",
            "blockClass": "txt-inst-title"
        }
    }, 
    
    "flex-layout.row#pp-txt-inst-principal": {
        "title": "Texto da página",
        "children": [      
            "rich-text#pp-txt-inst-principal"
        ] 
    }, 
    "flex-layout.row#pp-header-txt-inst": {
        "title": "Títulos da página",
        "children": [      
            "flex-layout.col#pp-txt-inst-titulo",
            "flex-layout.col#pp-txt-inst-subtitulo"
        ],
        "props": {         
            "blockClass": "header-default" 
        }
    },
    "flex-layout.col#pp-txt-inst-titulo": {
        "title": "Linha título da página",
        "children": [ "rich-text#pp-txt-inst-titulo" ],
        "props": { "width": "100%" }
    }, 
    "flex-layout.col#pp-txt-inst-subtitulo": {
        "title": "Linha subtítulo da página",
        "children": [ "rich-text#pp-txt-inst-subtitulo" ],
        "props": { "width": "100%" }
    }, 
    "rich-text#pp-txt-inst-titulo": {
        "title": "Título da página",
        "props": {
            "text": "## POLÍTICA DE PRIVACIDADE ##",
            "blockClass": "defaultBigTitle"
        }
    },
    "rich-text#pp-txt-inst-subtitulo": {
        "title": "Subtítulo da página",
        "props": {
            "text": "BEM-VINDO (A) AO UNIVERSO TETÊ CASTANHA!",
            "blockClass": "defaultSubtitle",
            "fullWidth": true
        }
    },
    "rich-text#pp-txt-inst-principal": {
        "title": "Texto da página",
        "props": {
            "text": "",
            "blockClass": "txt-inst-principal",
            "fullWidth": true
        }
    }
}
